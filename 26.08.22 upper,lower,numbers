#include<stdio.h>
#include<string.h>
void main()
{
	char c[20];
	printf("Enter any string\n");
	gets(c);
	int i,count1=0,count2=0,count3=0;
	for(i=0;c[i]!='\0';i++)
	{
		if(isupper(c[i]))
		{
			count1=count1+1;
		}
		else if(islower(c[i]))
		{
			count2=count2+1;
		}
		else if(isdigit(c[i]))
		{
			count3=count3+1;
		}
	}
	printf("%d",count1);
	printf("%d",count2);
	printf("%d",count3);
		
}
